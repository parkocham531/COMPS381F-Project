  <header>
    <H1>Create New Restaurant</H1>
  </header>
  <body>
    <form action="/fileupload" method="post" enctype="multipart/form-data">
      <br>
        Name<br><input type="text" name="name"><br>
        Borough<br><input type="text" name="borough"><br>
        Cuisine<br><input type="text" name="cuisine"><br>　　　　
        Street<br><input type="text" name="street"><br>
        Building<br><input type="text" name="building"><br>
        Zipcode<br><input type="text" name="zipcode"><br>
        Gpslat<br><input type="text" name="gpslat"><br>
        Gpslon<br><input type="text" name="gpslon"><br>
        <input type="file" name="filetoupload"><br>
        <br>
        <input type="submit">
    </form>
  </body>
</html>
